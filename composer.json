{
  "name": "mimmi20/laminasviewrenderer-bootstrap-navigation",
  "description": "Provides viewhelper for Laminas Navigation to use with Bootstrap",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation",
  "support": {
    "issues": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation/issues",
    "source": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation"
  },
  "require": {
    "php": "~8.3.0 || ~8.4.0 || ~8.5.0",
    "laminas/laminas-navigation": "^2.20.0",
    "laminas/laminas-stdlib": "^3.20.0",
    "laminas/laminas-view": "^2.36.0",
    "mimmi20/laminasviewrenderer-helper-htmlelement": "^3.0.0",
    "mimmi20/navigation-helper-acceptpage": "^4.0.1",
    "mimmi20/navigation-helper-containerparser": "^5.1.0",
    "mimmi20/navigation-helper-findactive": "^3.0.3",
    "mimmi20/navigation-helper-htmlify": "^5.0.2"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlreader": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.29.14",
    "laminas/laminas-authentication": "^2.18.0",
    "laminas/laminas-escaper": "^2.16.0",
    "laminas/laminas-i18n": "^2.29.0",
    "laminas/laminas-modulemanager": "^2.17.0",
    "laminas/laminas-mvc": "^3.8.0",
    "laminas/laminas-permissions-acl": "^2.17.0",
    "laminas/laminas-router": "^3.14.0",
    "laminas/laminas-servicemanager": "^3.22.1",
    "mimmi20/coding-standard": "^6.0.23",
    "nikic/php-parser": "^5.4.0",
    "phpstan/extension-installer": "^1.4.3",
    "phpstan/phpstan": "^2.1.11",
    "phpstan/phpstan-deprecation-rules": "^2.0.1",
    "phpunit/phpunit": "^12.1.0",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^2.0.11",
    "rector/type-perfect": "^2.0.2",
    "shipmonk/composer-dependency-analyser": "^1.8.2",
    "symfony/process": "^7.2.5",
    "symplify/phpstan-rules": "^14.6.0",
    "tomasvotruba/cognitive-complexity": "^1.0.0",
    "tomasvotruba/type-coverage": "^2.0.2",
    "tomasvotruba/unused-public": "^2.0.1"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-config": "to provide page configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-i18n": "required to use the Translator",
    "laminas/laminas-servicemanager": "to use the navigation factories",
    "mimmi20/mezzio-generic-authorization": "to use authrization roles or privileges"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\LaminasView\\BootstrapNavigation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\LaminasView\\BootstrapNavigation\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\LaminasView\\BootstrapNavigation",
      "config-provider": "Mimmi20\\LaminasView\\BootstrapNavigation\\ConfigProvider"
    }
  }
}
