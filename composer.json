{
  "name": "mimmi20/laminasviewrenderer-bootstrap-navigation",
  "description": "Provides viewhelper for Laminas Navigation to use with Bootstrap",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Thomas MÃ¼ller",
      "homepage": "https://github.com/mimmi20",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation",
  "support": {
    "issues": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation/issues",
    "source": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation"
  },
  "require": {
    "php": "~8.1.0 || ~8.2.0 || ~8.3.0 || ~8.4.0",
    "ext-mbstring": "*",
    "laminas/laminas-navigation": "^2.19.1",
    "laminas/laminas-stdlib": "^3.19.0",
    "laminas/laminas-view": "^2.35.0",
    "mimmi20/laminasviewrenderer-helper-htmlelement": "^2.0.8",
    "mimmi20/navigation-helper-acceptpage": "^2.1.3",
    "mimmi20/navigation-helper-containerparser": "^4.0.3",
    "mimmi20/navigation-helper-findactive": "^2.1.3",
    "mimmi20/navigation-helper-htmlify": "^4.0.3"
  },
  "require-dev": {
    "ext-ctype": "*",
    "ext-dom": "*",
    "ext-intl": "*",
    "ext-simplexml": "*",
    "ext-tokenizer": "*",
    "ext-xml": "*",
    "ext-xmlreader": "*",
    "ext-xmlwriter": "*",
    "infection/infection": "^0.27.11 || ^0.28.1",
    "laminas/laminas-authentication": "^2.17.0",
    "laminas/laminas-config": "^3.9.0",
    "laminas/laminas-escaper": "^2.13.0",
    "laminas/laminas-i18n": "^2.28.0",
    "laminas/laminas-modulemanager": "^2.16.0",
    "laminas/laminas-mvc": "^3.7.0",
    "laminas/laminas-permissions-acl": "^2.16.0",
    "laminas/laminas-router": "^3.13.0",
    "laminas/laminas-servicemanager": "^3.22.1",
    "mimmi20/coding-standard": "^5.2.40",
    "phpstan/extension-installer": "^1.4.1",
    "phpstan/phpstan": "^1.11.11",
    "phpstan/phpstan-deprecation-rules": "^1.2.0",
    "phpstan/phpstan-phpunit": "^1.4.0",
    "phpunit/phpunit": "^10.5.25",
    "psr/container": "^1.1.2 || ^2.0.2",
    "rector/rector": "^1.2.3",
    "rector/type-perfect": "^0.1.8",
    "symplify/phpstan-rules": "^13.0.0",
    "tomasvotruba/cognitive-complexity": "^0.2.3",
    "tomasvotruba/type-coverage": "^0.3.1",
    "tomasvotruba/unused-public": "^0.3.10"
  },
  "suggest": {
    "ext-intl": "required to use the Translator",
    "laminas/laminas-config": "to provide page configuration (optional, as arrays and Traversables are also allowed)",
    "laminas/laminas-i18n": "required to use the Translator",
    "laminas/laminas-servicemanager": "to use the navigation factories",
    "mimmi20/mezzio-generic-authorization": "to use authrization roles or privileges"
  },
  "autoload": {
    "psr-4": {
      "Mimmi20\\LaminasView\\BootstrapNavigation\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mimmi20Test\\LaminasView\\BootstrapNavigation\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true
    },
    "lock": false,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component": "Mimmi20\\LaminasView\\BootstrapNavigation",
      "config-provider": "Mimmi20\\LaminasView\\BootstrapNavigation\\ConfigProvider"
    }
  }
}
