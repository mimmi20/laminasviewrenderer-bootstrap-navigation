{
    "name": "mimmi20/laminasviewrenderer-bootstrap-navigation",
    "description": "Provides viewhelper for Laminas Navigation to use with Bootstrap",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas MÃ¼ller",
            "homepage": "https://github.com/mimmi20",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation",
    "support": {
        "issues": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation/issues",
        "source": "https://github.com/mimmi20/laminasviewrenderer-bootstrap-navigation"
    },
    "require": {
        "php": "^8.1.0",
        "ext-mbstring": "*",
        "laminas/laminas-navigation": "^2.15.0",
        "laminas/laminas-view": "^2.23.0",
        "mimmi20/laminasviewrenderer-helper-htmlelement": "^2.0.5",
        "mimmi20/navigation-helper-acceptpage": "^2.0.0",
        "mimmi20/navigation-helper-containerparser": "^3.0.2",
        "mimmi20/navigation-helper-findactive": "^2.0.0",
        "mimmi20/navigation-helper-findroot": "^2.0.0",
        "mimmi20/navigation-helper-htmlify": "^3.0.0"
    },
    "require-dev": {
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-intl": "*",
        "ext-simplexml": "*",
        "ext-tokenizer": "*",
        "ext-xml": "*",
        "ext-xmlwriter": "*",
        "infection/infection": "^0.27.7",
        "laminas/laminas-authentication": "^2.12.0",
        "laminas/laminas-config": "^3.7.0",
        "laminas/laminas-escaper": "^2.12.0",
        "laminas/laminas-i18n": "^2.17.0",
        "laminas/laminas-modulemanager": "^2.11.0",
        "laminas/laminas-mvc": "^3.7.0",
        "laminas/laminas-permissions-acl": "^2.10.0",
        "laminas/laminas-router": "^3.5.0",
        "laminas/laminas-servicemanager": "^3.10.0",
        "mimmi20/coding-standard": "^5.1.23",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^1.2.0",
        "phpstan/phpstan-deprecation-rules": "^1.0.0",
        "phpstan/phpstan-phpunit": "^1.3.15",
        "phpunit/phpunit": "^10.4.2",
        "rector/rector": "^0.18.8"
    },
    "suggest": {
        "ext-intl": "required to use the Translator",
        "laminas/laminas-config": "to provide page configuration (optional, as arrays and Traversables are also allowed)",
        "laminas/laminas-i18n": "required to use the Translator",
        "laminas/laminas-servicemanager": "to use the navigation factories",
        "mimmi20/mezzio-generic-authorization": "to use authrization roles or privileges"
    },
    "autoload": {
        "psr-4": {
            "Mimmi20\\LaminasView\\BootstrapNavigation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mimmi20Test\\LaminasView\\BootstrapNavigation\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "lock": false,
        "sort-packages": true,
        "allow-plugins": {
            "laminas/laminas-dependency-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "infection/extension-installer": true
        }
    },
    "extra": {
        "laminas": {
            "component": "Mimmi20\\LaminasView\\BootstrapNavigation",
            "config-provider": "Mimmi20\\LaminasView\\BootstrapNavigation\\ConfigProvider"
        }
    }
}
